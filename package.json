{
  "name": "@lmdc/react-gen-routes",
  "version": "0.9.0",
  "description": "Generate config for react-router-config based on directory architecture",
  "bin": {
    "lmdc-rgr": "./bin/index.js"
  },
  "main": "./index.js",
  "scripts": {
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch ./src ./src/ --exec 'npm run build'",
    "prebuild": "rm -rf ./dist",
    "postbuild": "cp package.json dist",
    "build": "cp -Rp ./src/ ./dist"
  },
  "homepage": "https://github.com/gtdudu/react-gen-routes#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gtdudu/react-gen-routes.git"
  },
  "keywords": [
    "react-router-config",
    "@loadable/component",
    "react",
    "next"
  ],
  "bugs": {
    "url": "https://github.com/gtdudu/react-gen-routes/issues"
  },
  "author": "tommy durand",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.9.0",
    "bluebird": "^3.7.2",
    "chokidar": "^3.3.1",
    "core-js": "^3.6.4",
    "ejs": "^3.0.1",
    "esm": "^3.2.25",
    "getopts": "^2.2.5",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.2",
    "react": "^16.13.1"
  }
}
